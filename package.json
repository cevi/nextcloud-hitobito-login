{
  "name": "hitobitologin",
  "version": "1.0.0",
  "description": "Hitobito login for Nextcloud",
  "license": "Apache-2.0",
  "type": "module",
  "scripts": {
    "build": "vite --mode production build",
    "dev": "NODE_ENV=development vite --mode development build",
    "dev:win": "set NODE_ENV=development && vite --mode development build",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint": "eslint --ext .js,.ts,.vue src",
    "lint:fix": "eslint --ext .js,.ts,.vue src --fix",
    "stylelint": "stylelint src/**/*{.scss,.vue,.css}",
    "stylelint:fix": "stylelint src --fix",
    "watch": "NODE_ENV=development vite --mode development build --watch",
    "watch:win": "set NODE_ENV=development && vite --mode development build --watch"
  },
  "browserslist": [
    "extends @nextcloud/browserslist-config"
  ],
  "dependencies": {
    "@nextcloud/axios": "^2.5.2",
    "@nextcloud/dialogs": "^7.0.0",
    "@nextcloud/initial-state": "^3.0.0",
    "@nextcloud/l10n": "^3.4.0",
    "@nextcloud/vue": "^9.0.1",
    "vue": "^3.5.22",
    "vue-material-design-icons": "^5.3.1"
  },
  "devDependencies": {
    "@nextcloud/browserslist-config": "^3.0.1",
    "@nextcloud/eslint-config": "^8.4.2",
    "@nextcloud/prettier-config": "^1.2.0",
    "@nextcloud/stylelint-config": "^3.1.1",
    "@nextcloud/vite-config": "^2.5.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "vite": "^7.1.9"
  },
  "engines": {
    "node": "^20.0.0",
    "npm": "^10.0.0"
  },
  "volta": {
    "node": "20.19.5"
  }
}
