{
    "name": "hitobitologin",
    "description": "Hitobito login for Nextcloud",
    "version": "1.0.0",
    "license": "Apache-2.0",
    "engines": {
        "node": "^20.0.0",
        "npm": "^10.0.0"
    },
    "scripts": {
        "build": "webpack --config webpack.config.js --node-env production --progress",
        "dev": "webpack --config webpack.config.js --node-env development --progress",
        "watch": "webpack --config webpack.config.js --node-env development --progress --watch",
        "lint": "eslint src",
